<div>
  <% if cards.count == 0 %>
    カードはまだありません．
  <% else %>
    <% cards.each do |c| %>
      <div>訪問日：<%= c.visit_date%></div>
      <div>Ver.<%= c.version_bf %>.<%= c.version_af %>(<%= c.version_y %>.<%= c.version_m %>)</div>
      <div class="">
        <%= attachment_image_tag c, :card_image, size: '200x150', format: 'jpeg', fallback: "dam_no_image.jpeg", class:"" %>
      </div>
    <% end %>
  <% end %>
</div>